{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",

    "TokenKey": "serge project key",

    "Serilog": {
        "Using": [],
        "MinimumLevel": {
            "Default": "Information",
            "Ovveride": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
        "WriteTo": [
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://seq:5341"
                }
            }
        ]
    }
}